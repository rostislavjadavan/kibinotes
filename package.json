    "typeface-source-sans-pro": "^1.1.13",
    "uuid": "^8.3.2",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
